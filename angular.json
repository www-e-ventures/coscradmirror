{
  "version": 1,
  "projects": {
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": [
              "apps/api/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/api/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/api"
          ],
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-interfaces": {
      "root": "libs/api-interfaces",
      "sourceRoot": "libs/api-interfaces/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/api-interfaces/**/*.ts"
            ]
          }
        }
      }
    },
    "tw-research": {
      "projectType": "application",
      "root": "apps/tw-research",
      "sourceRoot": "apps/tw-research/src",
      "prefix": "coscrad",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/tw-research",
            "index": "apps/tw-research/src/index.html",
            "main": "apps/tw-research/src/main.ts",
            "polyfills": "apps/tw-research/src/polyfills.ts",
            "tsConfig": "apps/tw-research/tsconfig.app.json",
            "assets": [
              "apps/tw-research/src/favicon.ico",
              "apps/tw-research/src/assets"
            ],
            "styles": [
              "apps/tw-research/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/tw-research/src/environments/environment.ts",
                  "with": "apps/tw-research/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "tw-research:build:production"
            },
            "development": {
              "browserTarget": "tw-research:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "apps/tw-research/proxy.conf.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "tw-research:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/tw-research/src/**/*.ts",
              "apps/tw-research/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/tw-research"
          ],
          "options": {
            "jestConfig": "apps/tw-research/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "tw-research-e2e": {
      "root": "apps/tw-research-e2e",
      "sourceRoot": "apps/tw-research-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/tw-research-e2e/cypress.json",
            "tsConfig": "apps/tw-research-e2e/tsconfig.e2e.json",
            "devServerTarget": "tw-research:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "tw-research:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/tw-research-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "nativescript-tng-dictionary-mobile": {
      "projectType": "application",
      "root": "apps/nativescript-tng-dictionary-mobile/",
      "sourceRoot": "apps/nativescript-tng-dictionary-mobile/src",
      "prefix": "",
      "architect": {
        "build": {
          "builder": "@nativescript/nx:build",
          "options": {
            "noHmr": true,
            "production": true,
            "uglify": true,
            "release": true,
            "forDevice": true
          },
          "configurations": {
            "prod": {
              "fileReplacements": [
                {
                  "replace": "./src/environments/environment.ts",
                  "with": "./src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "ios": {
          "builder": "@nativescript/nx:build",
          "options": {
            "platform": "ios"
          },
          "configurations": {
            "build": {
              "copyTo": "./dist/build.ipa"
            },
            "prod": {
              "combineWithConfig": "build:prod"
            }
          }
        },
        "android": {
          "builder": "@nativescript/nx:build",
          "options": {
            "platform": "android"
          },
          "configurations": {
            "build": {
              "copyTo": "./dist/build.apk"
            },
            "prod": {
              "combineWithConfig": "build:prod"
            }
          }
        },
        "clean": {
          "builder": "@nativescript/nx:build",
          "options": {
            "clean": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/nativescript-tng-dictionary-mobile/**/*.ts",
              "apps/nativescript-tng-dictionary-mobile/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/nativescript-tng-dictionary-mobile/jest.config.js",
            "tsConfig": "apps/nativescript-tng-dictionary-mobile/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/nativescript-tng-dictionary-mobile/src/test-setup.ts"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    }
  },
  "defaultProject": "tw-research"
}